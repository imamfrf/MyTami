<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EFEFEF"
    tools:context=".reimbursement.ReimbursementTravelItineraryActivity">

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appBarLayout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:theme="@style/ThemeOverlay.AppCompat.ActionBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?android:attr/actionBarSize"
            android:background="?attr/colorPrimary"
            app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
            app:titleTextColor="@color/md_white_1000" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:baselineAligned="false"
            android:gravity="center"
            android:orientation="horizontal"
            android:paddingStart="@dimen/padding_12dp"
            android:paddingEnd="@dimen/padding_12dp"
            android:paddingBottom="@dimen/padding_8dp">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:paddingTop="@dimen/padding_2dp"
                android:paddingBottom="@dimen/padding_2dp">

                <TextView
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:background="@drawable/oval_background_white"
                    android:fontFamily="sans-serif-condensed-light"
                    android:gravity="center"
                    android:text="1"
                    android:textColor="@color/colorPrimary"
                    android:textSize="12sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_4dp"
                    android:fontFamily="sans-serif-condensed-light"
                    android:text="Travel Type"
                    android:textColor="@color/md_white_1000"
                    android:textSize="12sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/margin_8dp"
                android:layout_marginEnd="@dimen/margin_8dp"
                android:layout_weight="1"
                android:background="@drawable/rounded_background_white"
                android:gravity="center"
                android:paddingTop="@dimen/padding_2dp"
                android:paddingBottom="@dimen/padding_2dp">

                <TextView
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:background="@drawable/oval_background_blue"
                    android:fontFamily="sans-serif-condensed"
                    android:gravity="center"
                    android:text="2"
                    android:textColor="@color/md_white_1000"
                    android:textSize="12sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_4dp"
                    android:fontFamily="sans-serif-condensed"
                    android:text="Travel Itinerary"
                    android:textColor="@color/colorPrimary"
                    android:textSize="12sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:paddingTop="@dimen/padding_2dp"
                android:paddingBottom="@dimen/padding_2dp">

                <TextView
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:background="@drawable/oval_background_white"
                    android:fontFamily="sans-serif-condensed-light"
                    android:gravity="center"
                    android:text="3"
                    android:textColor="@color/colorPrimary"
                    android:textSize="12sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_4dp"
                    android:fontFamily="sans-serif-condensed-light"
                    android:text="Description"
                    android:textColor="@color/md_white_1000"
                    android:textSize="12sp" />
            </LinearLayout>
        </LinearLayout>
    </com.google.android.material.appbar.AppBarLayout>

    <ScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appBarLayout">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_4dp"
                android:background="@color/md_white_1000"
                android:orientation="vertical"
                android:paddingTop="@dimen/padding_12dp"
                android:paddingBottom="@dimen/padding_8dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_16dp"
                    android:layout_marginBottom="@dimen/margin_8dp"
                    android:fontFamily="sans-serif-medium"
                    android:text="Event Information" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_white_1000"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingStart="0dp"
                    android:paddingEnd="@dimen/padding_16dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/padding_16dp"
                        android:src="@drawable/ic_calendar_start_date_27px" />

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/textInputLayout_start_date"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <EditText
                            android:id="@+id/editText_start_date"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="Start Date"
                            android:inputType="date"
                            android:textColor="@color/colorTextBlack" />

                    </com.google.android.material.textfield.TextInputLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_white_1000"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingStart="0dp"
                    android:paddingEnd="@dimen/padding_16dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/padding_16dp"
                        android:src="@drawable/ic_calendar_end_date_27px" />

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/textInputLayout_end_date"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <EditText
                            android:id="@+id/editText_end_date"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="End Date"
                            android:inputType="date"
                            android:textColor="@color/colorTextBlack" />

                    </com.google.android.material.textfield.TextInputLayout>
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_4dp"
                android:background="@color/md_white_1000"
                android:orientation="vertical"
                android:paddingTop="@dimen/padding_12dp"
                android:paddingBottom="@dimen/padding_12dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_16dp"
                    android:layout_marginBottom="@dimen/margin_8dp"
                    android:fontFamily="sans-serif-medium"
                    android:text="Trip Information" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_2dp"
                    android:background="@color/md_white_1000"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingStart="0dp"
                    android:paddingEnd="@dimen/padding_16dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/padding_16dp"
                        android:src="@drawable/ic_airplane_27px" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:paddingStart="@dimen/padding_4dp"
                        android:paddingEnd="@dimen/padding_4dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Type of Trip"
                            android:textColor="@color/colorTextGrey"
                            android:textSize="12sp" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/margin_8dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@drawable/rounded_background_white_stroke"
                                android:clickable="true"
                                android:focusable="true"
                                android:fontFamily="sans-serif-condensed"
                                android:gravity="center"
                                android:padding="@dimen/margin_8dp"
                                android:text="One Way"
                                android:textColor="@drawable/text_selector" />

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/margin_8dp"
                                android:layout_weight="1"
                                android:background="@drawable/rounded_background_white_stroke"
                                android:clickable="true"
                                android:focusable="true"
                                android:fontFamily="sans-serif-condensed"
                                android:gravity="center"
                                android:padding="@dimen/padding_8dp"
                                android:text="Round Trip"
                                android:textColor="@drawable/text_selector" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_12dp"
                    android:background="@color/md_white_1000"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingStart="0dp"
                    android:paddingEnd="@dimen/padding_16dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/padding_16dp"
                        android:src="@drawable/ic_train_27px" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:paddingStart="@dimen/padding_4dp"
                        android:paddingEnd="@dimen/padding_4dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Transport Type"
                            android:textColor="@color/colorTextGrey"
                            android:textSize="12sp" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/margin_8dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@drawable/rounded_background_white_stroke"
                                android:clickable="true"
                                android:focusable="true"
                                android:fontFamily="sans-serif-condensed"
                                android:gravity="center"
                                android:padding="@dimen/margin_8dp"
                                android:text="Kendaraan Umum"
                                android:textColor="@drawable/text_selector" />

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/margin_8dp"
                                android:layout_weight="1"
                                android:background="@drawable/rounded_background_white_stroke"
                                android:clickable="true"
                                android:focusable="true"
                                android:fontFamily="sans-serif-condensed"
                                android:gravity="center"
                                android:padding="@dimen/padding_8dp"
                                android:text="Kendaraan Dinas"
                                android:textColor="@drawable/text_selector" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_12dp"
                    android:background="@color/md_white_1000"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingStart="0dp"
                    android:paddingEnd="@dimen/padding_16dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/padding_16dp"
                        android:src="@drawable/ic_location_from_27px" />

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/textInputLayout_from"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <EditText
                            android:id="@+id/editText_from"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="From"
                            android:inputType="text"
                            android:textColor="@color/colorTextBlack" />

                    </com.google.android.material.textfield.TextInputLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_white_1000"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingStart="0dp"
                    android:paddingEnd="@dimen/padding_16dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/padding_16dp"
                        android:src="@drawable/ic_location_to_27px" />

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/textInputLayout_to"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <EditText
                            android:id="@+id/editText_to"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="To"
                            android:inputType="text"
                            android:textColor="@color/colorTextBlack" />

                    </com.google.android.material.textfield.TextInputLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_white_1000"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingStart="0dp"
                    android:paddingEnd="@dimen/padding_16dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/padding_16dp"
                        android:src="@drawable/ic_calendar_departure_date_27px" />

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/textInputLayout_departure_date"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <EditText
                            android:id="@+id/editText_departure_date"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="Departure"
                            android:inputType="date"
                            android:textColor="@color/colorTextBlack" />

                    </com.google.android.material.textfield.TextInputLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:paddingStart="59dp"
                    android:paddingEnd="@dimen/padding_16dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingEnd="@dimen/padding_4dp">

                        <CheckBox
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="H-1"
                            android:textColor="@color/colorPrimary" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentEnd="true"
                            android:layout_centerInParent="true"
                            android:clickable="true"
                            android:focusable="true"
                            android:foreground="?attr/selectableItemBackground"
                            android:src="@drawable/ic_information_20px" />
                    </RelativeLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/textInputLayout_reason_departure"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:visibility="gone">

                        <EditText
                            android:id="@+id/editText_reason_departure"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="Reason"
                            android:inputType="text"
                            android:textColor="@color/colorTextBlack" />

                    </com.google.android.material.textfield.TextInputLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_white_1000"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingStart="0dp"
                    android:paddingEnd="@dimen/padding_16dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/padding_16dp"
                        android:src="@drawable/ic_calendar_return_date_27px" />

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/textInputLayout_return_date"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <EditText
                            android:id="@+id/editText_return_date"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="Return Date"
                            android:inputType="date"
                            android:textColor="@color/colorTextBlack" />

                    </com.google.android.material.textfield.TextInputLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:paddingStart="59dp"
                    android:paddingEnd="@dimen/padding_16dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingEnd="@dimen/padding_4dp">

                        <CheckBox
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="H-1"
                            android:textColor="@color/colorPrimary" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentEnd="true"
                            android:layout_centerInParent="true"
                            android:clickable="true"
                            android:focusable="true"
                            android:foreground="?attr/selectableItemBackground"
                            android:src="@drawable/ic_information_20px" />
                    </RelativeLayout>

                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/textInputLayout_reason_return"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:visibility="gone">

                        <EditText
                            android:id="@+id/editText_reason_return"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="Reason"
                            android:inputType="text"
                            android:textColor="@color/colorTextBlack" />

                    </com.google.android.material.textfield.TextInputLayout>
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_4dp"
                android:background="@color/md_white_1000"
                android:orientation="vertical"
                android:paddingTop="@dimen/padding_12dp"
                android:paddingBottom="@dimen/padding_10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_16dp"
                    android:layout_marginBottom="@dimen/margin_8dp"
                    android:fontFamily="sans-serif-medium"
                    android:text="Facilities" />

                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_12dp"
                    android:text="Hotel"
                    android:textColor="@color/colorPrimary" />

                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_12dp"
                    android:text="Additional Transportation"
                    android:textColor="@color/colorPrimary" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_4dp"
                android:background="@color/md_white_1000"
                android:orientation="vertical"
                android:paddingTop="@dimen/padding_12dp"
                android:paddingBottom="@dimen/padding_10dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_16dp"
                    android:layout_marginBottom="@dimen/margin_8dp"
                    android:fontFamily="sans-serif-medium"
                    android:text="Others" />

                <CheckBox
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_12dp"
                    android:text="Other Cost Center (Khusus kegiatan training menggunakan Cost Center HC)"
                    android:textColor="@color/colorPrimary" />

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/textInputLayout_cost_center"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="40dp"
                    android:layout_marginTop="@dimen/margin_4dp"
                    android:paddingStart="0dp"
                    android:paddingEnd="@dimen/padding_16dp"
                    android:visibility="gone">

                    <EditText
                        android:id="@+id/editText_cost_center"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Cost Center"
                        android:inputType="text"
                        android:textColor="@color/colorTextBlack" />

                </com.google.android.material.textfield.TextInputLayout>

                <CheckBox
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_12dp"
                    android:layout_marginTop="@dimen/margin_8dp"
                    android:text="Diskresi (Dipilih apabila karyawan membutuhkan fasilitas diluar limit ketentuan perusahaan)"
                    android:textColor="@color/colorPrimary" />

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/textInputLayout_diskresi_type"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="40dp"
                    android:layout_marginTop="@dimen/margin_4dp"
                    android:paddingStart="0dp"
                    android:paddingEnd="@dimen/padding_16dp"
                    android:visibility="gone">

                    <EditText
                        android:id="@+id/editText_diskresi_type"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Diskresi Type"
                        android:inputType="text"
                        android:textColor="@color/colorTextBlack" />

                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/textInputLayout_diskresi_reason"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="40dp"
                    android:paddingStart="0dp"
                    android:paddingEnd="@dimen/padding_16dp"
                    android:visibility="gone">

                    <EditText
                        android:id="@+id/editText_diskresi_reason"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Reason"
                        android:inputType="text"
                        android:textColor="@color/colorTextBlack" />

                </com.google.android.material.textfield.TextInputLayout>

                <CheckBox
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_12dp"
                    android:layout_marginTop="@dimen/margin_8dp"
                    android:text="Makan malam ditanggung (Dipilih apabila mendapatkan fasilitas)"
                    android:textColor="@color/colorPrimary" />
            </LinearLayout>

            <Button
                android:id="@+id/button_next"
                style="@style/AppTheme.RoundedCornerMaterialButton"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/margin_10dp"
                android:text="Next"
                android:textAllCaps="false"
                android:textSize="16sp" />
        </LinearLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>