<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EFEFEF"
    tools:context=".TravelRequestTravelDescriptionActivity">

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appBarLayout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:theme="@style/ThemeOverlay.AppCompat.ActionBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?android:attr/actionBarSize"
            android:background="?attr/colorPrimary"
            app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
            app:titleTextColor="@color/md_white_1000" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:baselineAligned="false"
            android:gravity="center"
            android:orientation="horizontal"
            android:paddingStart="@dimen/padding_12dp"
            android:paddingEnd="@dimen/padding_12dp"
            android:paddingBottom="@dimen/padding_8dp">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:paddingTop="@dimen/padding_2dp"
                android:paddingBottom="@dimen/padding_2dp">

                <TextView
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:background="@drawable/oval_background_white"
                    android:fontFamily="sans-serif-condensed-light"
                    android:gravity="center"
                    android:text="1"
                    android:textColor="@color/colorPrimary"
                    android:textSize="12sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_4dp"
                    android:fontFamily="sans-serif-condensed-light"
                    android:text="Travel Type"
                    android:textColor="@color/md_white_1000"
                    android:textSize="12sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/margin_8dp"
                android:layout_marginEnd="@dimen/margin_8dp"
                android:layout_weight="1"
                android:gravity="center"
                android:paddingTop="@dimen/padding_2dp"
                android:paddingBottom="@dimen/padding_2dp">

                <TextView
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:background="@drawable/oval_background_white"
                    android:fontFamily="sans-serif-condensed-light"
                    android:gravity="center"
                    android:text="2"
                    android:textColor="@color/colorPrimary"
                    android:textSize="12sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_4dp"
                    android:fontFamily="sans-serif-condensed-light"
                    android:text="Travel Itinerary"
                    android:textColor="@color/md_white_1000"
                    android:textSize="12sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/rounded_background_white"
                android:gravity="center"
                android:paddingTop="@dimen/padding_2dp"
                android:paddingBottom="@dimen/padding_2dp">

                <TextView
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:background="@drawable/oval_background_blue"
                    android:fontFamily="sans-serif-condensed"
                    android:gravity="center"
                    android:text="3"
                    android:textColor="@color/md_white_1000"
                    android:textSize="12sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_4dp"
                    android:fontFamily="sans-serif-condensed"
                    android:text="Description"
                    android:textColor="@color/colorPrimary"
                    android:textSize="12sp" />
            </LinearLayout>
        </LinearLayout>
    </com.google.android.material.appbar.AppBarLayout>

    <ScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appBarLayout">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_4dp"
                android:background="@color/md_white_1000"
                android:orientation="vertical"
                android:paddingTop="@dimen/padding_12dp"
                android:paddingBottom="@dimen/padding_12dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_16dp"
                    android:layout_marginBottom="@dimen/margin_8dp"
                    android:fontFamily="sans-serif-medium"
                    android:text="Request Detail" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_white_1000"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingStart="0dp"
                    android:paddingEnd="@dimen/padding_16dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingStart="@dimen/padding_16dp"
                        android:paddingEnd="@dimen/padding_10dp"
                        android:src="@drawable/ic_id_travel_22px" />

                    <TextView
                        android:id="@+id/textView_travel_id"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="TEL742346816264348"
                        android:textColor="@color/colorPrimary" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_white_1000"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingStart="0dp"
                    android:paddingEnd="@dimen/padding_16dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingStart="@dimen/padding_16dp"
                        android:paddingEnd="@dimen/padding_10dp"
                        android:src="@drawable/ic_calendar_22px" />

                    <TextView
                        android:id="@+id/textView_travel_request_date"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="20 June 2019"
                        android:textColor="@color/colorPrimary" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_white_1000"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingStart="0dp"
                    android:paddingEnd="@dimen/padding_16dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingStart="@dimen/padding_16dp"
                        android:paddingEnd="@dimen/padding_10dp"
                        android:src="@drawable/ic_id_card_22px" />

                    <TextView
                        android:id="@+id/textView_id"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="telin1"
                        android:textColor="@color/colorPrimary" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_white_1000"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingStart="0dp"
                    android:paddingEnd="@dimen/padding_16dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingStart="@dimen/padding_16dp"
                        android:paddingEnd="@dimen/padding_10dp"
                        android:src="@drawable/ic_airplane_22px" />

                    <TextView
                        android:id="@+id/textView_type_of_trip"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Round Trip"
                        android:textColor="@color/colorPrimary" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_white_1000"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingStart="0dp"
                    android:paddingEnd="@dimen/padding_16dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingStart="@dimen/padding_16dp"
                        android:paddingEnd="@dimen/padding_10dp"
                        android:src="@drawable/ic_additional_22px" />

                    <TextView
                        android:id="@+id/textView_additional"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Other Cost Center, Diskresi"
                        android:textColor="@color/colorPrimary" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_4dp"
                android:background="@color/md_white_1000"
                android:orientation="vertical"
                android:paddingTop="@dimen/padding_12dp"
                android:paddingBottom="@dimen/padding_12dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_16dp"
                    android:layout_marginBottom="@dimen/margin_8dp"
                    android:fontFamily="sans-serif-medium"
                    android:text="Travel Information" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_16dp"
                    android:text="Purpose and Objectives"
                    android:textColor="@color/colorPrimary"
                    android:textSize="12sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_16dp"
                    android:text="Meeting Client Starbucks"
                    android:textColor="#4A4A4A" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_16dp"
                    android:layout_marginTop="@dimen/margin_8dp"
                    android:text="Note"
                    android:textColor="@color/colorPrimary"
                    android:textSize="12sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_16dp"
                    android:text="Kerjasama perusahaan"
                    android:textColor="#4A4A4A" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_4dp"
                android:background="@color/md_white_1000"
                android:orientation="vertical"
                android:paddingTop="@dimen/padding_12dp"
                android:paddingBottom="@dimen/padding_12dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_16dp"
                    android:layout_marginBottom="@dimen/margin_8dp"
                    android:fontFamily="sans-serif-medium"
                    android:text="Event Detail" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_white_1000"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingStart="0dp"
                    android:paddingEnd="@dimen/padding_16dp">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/margin_16dp"
                            android:fontFamily="@font/lato_bold"
                            android:text="Start Date"
                            android:textColor="@color/colorPrimary"
                            android:textSize="16sp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/margin_16dp"
                            android:layout_marginTop="@dimen/margin_4dp"
                            android:fontFamily="@font/lato_light"
                            android:text="Monday, 24 June 2019"
                            android:textColor="@color/colorPrimary"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/img_event_arrow" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/margin_16dp"
                            android:fontFamily="@font/lato_bold"
                            android:text="End Date"
                            android:textColor="@color/colorPrimary"
                            android:textSize="16sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/margin_16dp"
                            android:layout_marginTop="@dimen/margin_4dp"
                            android:fontFamily="@font/lato_light"
                            android:text="Thursday, 27 June 2019"
                            android:textColor="@color/colorPrimary"
                            android:textSize="12sp" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_4dp"
                android:background="@color/md_white_1000"
                android:orientation="vertical"
                android:paddingTop="@dimen/padding_12dp"
                android:paddingBottom="@dimen/padding_12dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_16dp"
                    android:layout_marginBottom="@dimen/margin_8dp"
                    android:fontFamily="sans-serif-medium"
                    android:text="Trip Detail" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/md_white_1000"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingStart="0dp"
                    android:paddingEnd="@dimen/padding_16dp">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/margin_16dp"
                            android:fontFamily="@font/lato_bold"
                            android:text="Jakarta"
                            android:textColor="@color/colorPrimary"
                            android:textSize="16sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/margin_16dp"
                            android:layout_marginTop="@dimen/margin_4dp"
                            android:fontFamily="@font/lato_light"
                            android:text="Depart Date"
                            android:textColor="@color/colorPrimary"
                            android:textSize="12sp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/margin_16dp"
                            android:layout_marginTop="@dimen/margin_4dp"
                            android:fontFamily="@font/lato_light"
                            android:text="Sunday, 23 June 2019"
                            android:textColor="@color/colorPrimary"
                            android:textSize="12sp" />
                    </LinearLayout>

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/img_trip_arrow" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/margin_16dp"
                            android:fontFamily="@font/lato_bold"
                            android:text="Surabaya"
                            android:textColor="@color/colorPrimary"
                            android:textSize="16sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/margin_16dp"
                            android:layout_marginTop="@dimen/margin_4dp"
                            android:fontFamily="@font/lato_light"
                            android:text="Return Date"
                            android:textColor="@color/colorPrimary"
                            android:textSize="12sp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/margin_16dp"
                            android:layout_marginTop="@dimen/margin_4dp"
                            android:fontFamily="@font/lato_light"
                            android:text="Thursday, 27 June 2019"
                            android:textColor="@color/colorPrimary"
                            android:textSize="12sp" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/margin_8dp"
                android:background="@drawable/rounded_background_blue_stroke"
                android:orientation="vertical"
                android:padding="@dimen/padding_16dp">

                <RelativeLayout
                    android:id="@+id/expand_button"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:foreground="?selectableItemBackground">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:fontFamily="@font/lato_bold"
                        android:text="Total"
                        android:textColor="@color/md_white_1000"
                        android:textSize="18sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="28dp"
                        android:fontFamily="@font/lato_bold"
                        android:text="IDR7,660,000"
                        android:textColor="@color/md_white_1000"
                        android:textSize="18sp" />

                    <ImageView
                        android:id="@+id/imageView_expand_arrow"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/ic_keyboard_arrow_down_grey_24dp"
                        android:tint="@color/md_white_1000" />
                </RelativeLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="@dimen/margin_8dp"
                    android:background="@color/md_white_1000" />

                <net.cachapa.expandablelayout.ExpandableLayout
                    android:id="@+id/expandable_layout_0"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_8dp"
                    app:el_duration="1000"
                    app:el_expanded="false"
                    app:el_parallax="0.5">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:clipToPadding="false"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:id="@+id/relativeLayout_plane"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:id="@+id/textView_plane"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/lato_regular"
                                android:text="Plane"
                                android:textColor="@color/md_white_1000" />

                            <TextView
                                android:id="@+id/textView_plane_to"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/textView_plane"
                                android:layout_centerVertical="true"
                                android:layout_marginStart="@dimen/margin_8dp"
                                android:layout_marginTop="@dimen/margin_4dp"
                                android:fontFamily="@font/lato_light"
                                android:text="Jakarta to Surabaya"
                                android:textColor="@color/md_white_1000"
                                android:textSize="12sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/textView_plane"
                                android:layout_alignParentEnd="true"
                                android:layout_marginTop="@dimen/margin_4dp"
                                android:layout_marginEnd="@dimen/margin_8dp"
                                android:fontFamily="@font/lato_light"
                                android:text="IDR1,500,000"
                                android:textColor="@color/md_white_1000"
                                android:textSize="12sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/textView_plane_to"
                                android:layout_centerVertical="true"
                                android:layout_marginStart="@dimen/margin_8dp"
                                android:layout_marginTop="@dimen/margin_4dp"
                                android:fontFamily="@font/lato_light"
                                android:text="Surabaya to Jakarta"
                                android:textColor="@color/md_white_1000"
                                android:textSize="12sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/textView_plane_to"
                                android:layout_alignParentEnd="true"
                                android:layout_marginTop="@dimen/margin_4dp"
                                android:layout_marginEnd="@dimen/margin_8dp"
                                android:fontFamily="@font/lato_light"
                                android:text="IDR1,800,000"
                                android:textColor="@color/md_white_1000"
                                android:textSize="12sp" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/relativeLayout_tarif_penginapan"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/margin_8dp">

                            <TextView
                                android:id="@+id/textView_tarif_penginapan"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/lato_regular"
                                android:text="Tarif Penginapan"
                                android:textColor="@color/md_white_1000" />

                            <TextView
                                android:id="@+id/textView_tarif_penginapan_tempat"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/textView_tarif_penginapan"
                                android:layout_centerVertical="true"
                                android:layout_marginStart="@dimen/margin_8dp"
                                android:layout_marginTop="@dimen/margin_4dp"
                                android:fontFamily="@font/lato_light"
                                android:text="Surabaya (IDR500,000 x 4 nights)"
                                android:textColor="@color/md_white_1000"
                                android:textSize="12sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/textView_tarif_penginapan"
                                android:layout_alignParentEnd="true"
                                android:layout_marginTop="@dimen/margin_4dp"
                                android:layout_marginEnd="@dimen/margin_8dp"
                                android:fontFamily="@font/lato_light"
                                android:text="IDR2,000,000"
                                android:textColor="@color/md_white_1000"
                                android:textSize="12sp" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:id="@+id/relativeLayout_bantuan_transportasi"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/margin_8dp">

                            <TextView
                                android:id="@+id/textView_bantuan_transportasi"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/lato_regular"
                                android:text="Bantuan Transportasi Bandara"
                                android:textColor="@color/md_white_1000" />

                            <TextView
                                android:id="@+id/textView_bantuan_transportasi_tempat"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/textView_bantuan_transportasi"
                                android:layout_centerVertical="true"
                                android:layout_marginStart="@dimen/margin_8dp"
                                android:layout_marginTop="@dimen/margin_4dp"
                                android:fontFamily="@font/lato_light"
                                android:text="Jakarta"
                                android:textColor="@color/md_white_1000"
                                android:textSize="12sp" />

                            <TextView
